server {
    listen 80 default_server;
    listen [::]:80 default_server;

    server_name 54.236.125.12

    add_header X-Served-BY 218560-web-01;

    location = /airbnb-onepage/  {
        proxy_pass https://127.0.0.1:5000/airbnb-onepage/;
    }

    error_page 404 /404.html;
    location /404  {
      root /var/www/html;
      internal;
    }
}
